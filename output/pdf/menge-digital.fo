	<fo:block id="id44"><fo:block><fo:block><fo:block keep-together.within-column="always" margin-left="0pt" font-family="GFS Didot,Symbol,ZapfDingbats"><fo:block keep-with-next.within-column="always"><fo:block font-family="GFS Didot,Symbol,ZapfDingbats" font-weight="bold" keep-with-next.within-column="always" space-before.minimum="0.8em" space-before.optimum="1.0em" space-before.maximum="1.2em" text-align="start" start-indent="0pt"><fo:marker marker-class-name="section.head.marker">K. Konjunktionen.</fo:marker><fo:block font-size="19.2pt" font-weight="normal" text-transform="uppercase">K. Konjunktionen.</fo:block></fo:block></fo:block></fo:block></fo:block></fo:block>
		
		<fo:block space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em">
	<fo:inline font-weight="bold" keep-with-next.within-line="always" padding-end="1em">499. </fo:inline>
